{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Tester",
			"program": "${workspaceFolder}/test.js",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "pwa-node"
		},

	  {
		"name": "Debug Main Process",
		"type": "node",
		"request": "launch",
		"cwd": "${workspaceFolder}",
		"runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
		"windows": {
		  "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
		},
		"args" : ["."],
		"outputCapture": "std"
	  }
	]
}